setTimeout(function() {
	
	<% if @ancestors.empty? %>
		$('#done').foundation('reveal', 'open')
	<% else %>
		$('#found').foundation('reveal', 'open')
		$('#ancestors').empty()
		$('#ancestors').append("<%= j render :partial => 'ancestor', :collection => @ancestors, :as => :ancestor %>");
	<% end %>
}, 500);